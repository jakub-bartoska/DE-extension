{
  "manifest_version": 3,
  "name": "DE-extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.darkelf.cz/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/menu-icon.png"
      ],
      "matches": [
        "https://www.darkelf.cz/*"
      ]
    }
  ]
}